=\"mx-auto h-16 w-16 bg-emerald-600 rounded-xl flex items-center justify-center shadow-lg\">\n          <BuildingStorefrontIcon class=\"h-8 w-8 text-white\" />\n        </div>\n        <h2 class=\"mt-6 text-3xl font-bold text-gray-900\">Bem vindo de Volta</h2>\n        <p class=\"mt-2 text-sm text-gray-600\">Faça login na sua conta de Administrador ComandaGo</p>\n      </div>\n\n      <!-- Login Form -->\n      <div class=\"card\">\n        <div class=\"card-body\">\n          <form @submit.prevent=\"handleLogin\" class=\"space-y-6\">\n            <div>\n              <label for=\"email\" class=\"block text-sm font-medium text-gray-700 mb-2\">\n                Endereço de e-mail / Usuário\n              </label>\n              <input\n                id=\"email\"\n                v-model=\"form.email\"\n                type=\"email\"\n                autocomplete=\"email\"\n                required\n                class=\"input-field\"\n                :class=\"{ 'input-error': errors.email }\"\n                placeholder=\"admin@comandago.com\"\n              />\n              <p v-if=\"errors.email\" class=\"mt-1 text-sm text-red-600\">{{ errors.email }}</p>\n            </div>\n\n            <div>\n              <label for=\"password\" class=\"block text-sm font-medium text-gray-700 mb-2\">\n                Senha\n              </label>\n              <div class=\"relative\">\n                <input\n                  id=\"password\"\n                  v-model=\"form.password\"\n                  :type=\"showPassword ? 'text' : 'password'\"\n                  autocomplete=\"current-password\"\n                  required\n                  class=\"input-field pr-10\"\n                  :class=\"{ 'input-error': errors.password }\"\n                  placeholder=\"Digite sua senha\"\n                />\n                <button\n                  type=\"button\"\n                  @click=\"showPassword = !showPassword\"\n                  class=\"absolute inset-y-0 right-0 pr-3 flex items-center\"\n                >\n                  <EyeIcon v-if=\"!showPassword\" class=\"h-5 w-5 text-gray-400 hover:text-gray-600\" />\n                  <EyeSlashIcon v-else class=\"h-5 w-5 text-gray-400 hover:text-gray-600\" />\n                </button>\n              </div>\n              <p v-if=\"errors.password\" class=\"mt-1 text-sm text-red-600\">{{ errors.password }}</p>\n            </div>\n\n            <div class=\"flex items-center justify-between\">\n              <div class=\"flex items-center\">\n                <input\n                  id=\"remember-me\"\n                  v-model=\"form.rememberMe\"\n                  type=\"checkbox\"\n                  class=\"h-4 w-4 text-emerald-600 focus:ring-emerald-500 border-gray-300 rounded\"\n                />\n                <label for=\"remember-me\" class=\"ml-2 block text-sm text-gray-700\">\n                  Remember me\n                </label>\n              </div>\n\n              <div class=\"text-sm\">\n                <a href=\"#\" class=\"font-medium text-emerald-600 hover:text-emerald-500 transition-colors\">\n                  Forgot your password?\n                </a>\n              </div>\n            </div>\n\n            <div v-if=\"loginError\" class=\"bg-red-50 border border-red-200 rounded-lg p-4\">\n              <div class=\"flex\">\n                <ExclamationTriangleIcon class=\"h-5 w-5 text-red-400\" />\n                <div class=\"ml-3\">\n                  <p class=\"text-sm text-red-800\">{{ loginError }}</p>\n                </div>\n              </div>\n            </div>\n\n            <button\n              type=\"submit\"\n              :disabled=\"authStore.isLoading\"\n              class=\"w-full btn-primary py-3 text-base font-semibold\"\n            >\n              <div v-if=\"authStore.isLoading\" class=\"flex items-center justify-center\">\n                <div class=\"loading-spinner mr-2\"></div>\n                Signing in...\n              </div>\n              <span v-else>Sign in</span>\n            </button>\n          </form>\n        </div>\n      </div>\n\n      <!-- Demo Credentials -->\n      <div class=\"bg-blue-50 border border-blue-200 rounded-lg p-4\">\n        <div class=\"flex\">\n          <InformationCircleIcon class=\"h-5 w-5 text-blue-400 mt-0.5\" />\n          <div class=\"ml-3\">\n            <h3 class=\"text-sm font-medium text-blue-800\">Demo Credentials</h3>\n            <div class=\"mt-2 text-sm text-blue-700\">\n              <p><strong>Email:</strong> admin@comandago.com</p>\n              <p><strong>Password:</strong> admin123</p>\n            </div>\n          </div>\n        </div>\n      </div>\n\n      <!-- Footer -->\n      <div class=\"text-center\">\n        <p class=\"text-xs text-gray-500\">\n          © 2025 ComandaGo Admin. Professional Restaurant Management System.\n        </p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script setup>\nimport { \n  BuildingStorefrontIcon, \n  EyeIcon, \n  EyeSlashIcon, \n  ExclamationTriangleIcon,\n  InformationCircleIcon \n} from '@heroicons/vue/24/outline'\n\ndefinePageMeta({\n  layout: false\n})\n\nconst authStore = useAuthStore()\n\nconst form = ref({\n  email: '',\n  password: '',\n  rememberMe: false\n})\n\nconst errors = ref({})\nconst loginError = ref('')\nconst showPassword = ref(false)\n\nconst validateLoginForm = () => {\n  errors.value = {}\n  \n  if (!form.value.email) {\n    errors.value.email = 'Email is required'\n  } else if (!/\\S+@\\S+\\.\\S+/.test(form.value.email)) {\n    errors.value.email = 'Please enter a valid email address'\n  }\n  \n  if (!form.value.password) {\n    errors.value.password = 'Password is required'\n  } else if (form.value.password.length < 6) {\n    errors.value.password = 'Password must be at least 6 characters'\n  }\n  \n  return Object.keys(errors.value).length === 0\n}\n\nconst handleLogin = async () => {\n  loginError.value = ''\n  \n  if (!validateLoginForm()) {\n    return\n  }\n  \n  const result = await authStore.login({\n    email: form.value.email,\n    password: form.value.password\n  })\n  \n  if (!result.success) {\n    loginError.value = result.error\n  }\n}\n\n// Auto-fill demo credentials for easier testing\nonMounted(() => {\n  form.value.email = 'admin@comandago.com'\n  form.value.password = 'admin123'\n})\n</script>"],"version":3}